{% extends 'base.html.twig' %}

{% block title %}Nouvelle annonce{% endblock %}

{% block body %}
    <section class="page-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading text-uppercase">Publier une annonce</h2>
                </div>
                <div class="col-lg-12">
                    {{ form_start(form) }}
                        {{ form_widget(form) }}
                        <button class="btn btn-success">{{ button_label|default('Save') }}</button>
                    {{ form_end(form) }}

                    <a class="btn btn-primary" href="{{ path('front_office') }}">Retour</a>
                </div>
            </div>
        </div>
    </section>
{% endblock %}
