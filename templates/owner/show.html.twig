{% extends 'base.html.twig' %}

{% block title %}
Owner
{% endblock %}


{% block body %}
<section class="page-section">
	<div class="container">
		<div class="row">
			<div class="col-lg-12 text-center">
				<h2 class="section-heading text-uppercase">Client</h2>
			</div>
			<div class="col-lg-12">
				<table class="table">
					<tbody>
						<tr>
							<th>Id</th>
							<td>{{ owner.id }}</td>
						</tr>
						<tr>
							<th>Address</th>
							<td>{{ owner.address }}</td>
						</tr>
						<tr>
							<th>Country</th>
							<td>{{ owner.country }}</td>
						</tr>
					</tbody>
				</table>

				<a class="btn btn-primary" href="{{ path('owner_index') }}">back
					to list</a> <a class="btn btn-secondary"
					href="{{ path('owner_edit', {'id': owner.id}) }}">edit</a>

				{{ include('owner/_delete_form.html.twig') }}
			</div>
		</div>
	</div>
</section>
{% endblock %}
